  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run server",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --watch-poll=800",
    "check": "npm run check:js && npm run check:ts",
    "check:js": "git status -s | grep \"src\" | grep -v \"src/lib\" | grep -v \".ts\" | grep -v \".json\" | (grep \".js\" || echo --version) | awk '{print $NF}' | xargs -n1 node node_modules/eslint/bin/eslint.js --fix",
    "check:ts": "git status -s | grep \"src\" | (grep \".ts\" || echo --version) | awk '{print $NF}' | xargs -n1 node node_modules/tslint/bin/tslint"
  },
